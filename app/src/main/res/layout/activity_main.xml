<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/fond_bureau"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/background_sm_169"
        android:scaleType="centerCrop"
        />


    <ImageView
        android:layout_width="67dp"
        android:layout_height="101dp"
        android:layout_marginStart="186dp"
        android:layout_marginTop="247dp"
        android:layout_marginEnd="478dp"
        android:layout_marginBottom="63dp"
        android:src="@drawable/dossier"
        app:layout_constraintBottom_toBottomOf="@+id/fond_bureau"
        app:layout_constraintEnd_toEndOf="@+id/fond_bureau"
        app:layout_constraintStart_toStartOf="@+id/fond_bureau"
        app:layout_constraintTop_toTopOf="@+id/fond_bureau" />

    <ImageButton
        android:id="@+id/radio"
        android:layout_width="168dp"
        android:layout_height="138dp"
        android:layout_marginStart="384dp"
        android:layout_marginTop="171dp"


        android:layout_marginEnd="179dp"
        android:layout_marginBottom="102dp"
        android:background="@drawable/radio"
        app:layout_constraintBottom_toBottomOf="@+id/fond_bureau"
        app:layout_constraintEnd_toEndOf="@+id/fond_bureau"
        app:layout_constraintStart_toStartOf="@+id/fond_bureau"
        app:layout_constraintTop_toTopOf="@+id/fond_bureau" />


</androidx.constraintlayout.widget.ConstraintLayout>